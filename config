ngx_feature="SDCH module"
ngx_feature_libs="-lvcdcom -lvcdenc"
ngx_feature_name=
ngx_feature_run=no
ngx_feature_incs=""
ngx_feature_path=
ngx_feature_test=""

    # auto-discovery
    . auto/feature


if [ $ngx_found = yes ]; then
    CORE_INCS="$CORE_INCS $ngx_feature_path"
    CORE_LIBS="$CORE_LIBS $ngx_feature_libs"
else
 cat << END
 $0: error: SDCH feature not found.
END
 exit 1
fi

ngx_addon_name=sdch_module
HTTP_AUX_FILTER_MODULES="$HTTP_AUX_FILTER_MODULES sdch_module"

NGX_ADDON_SRCS="$NGX_ADDON_SRCS \
                $ngx_addon_dir/sdch_module.c \
                $ngx_addon_dir/vcd-h1.cc \
                $ngx_addon_dir/ngoustr.cc \
                $ngx_addon_dir/teefd.cpp \
                $ngx_addon_dir/blobstore.cc \
                $ngx_addon_dir/blob.cc \
                "

NGX_ADDON_DEPS="$NGX_ADDON_DEPS \
                "


CORE_LIBS="$CORE_LIBS \
          -lvcdcom -lvcdenc \
          -lstdc++ \
	  "
